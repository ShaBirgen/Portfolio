#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Coding/React/React Projects/Portfolio/node_modules/loose-envify/node_modules:/mnt/e/Coding/React/React Projects/Portfolio/node_modules:/mnt/e/Coding/React/React Projects/node_modules:/mnt/e/Coding/React/node_modules:/mnt/e/Coding/node_modules:/mnt/e/node_modules:/mnt/e/Coding/React/React Projects/Portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Coding/React/React Projects/Portfolio/node_modules/loose-envify/node_modules:/mnt/e/Coding/React/React Projects/Portfolio/node_modules:/mnt/e/Coding/React/React Projects/node_modules:/mnt/e/Coding/React/node_modules:/mnt/e/Coding/node_modules:/mnt/e/node_modules:/mnt/e/Coding/React/React Projects/Portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
